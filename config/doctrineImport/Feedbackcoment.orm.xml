<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Feedbackcoment" table="feedbackcoment">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="codCasa" type="integer" column="cod_casa" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="coment" type="string" column="coment" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="valorVoto" type="integer" column="valor_voto" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="reserva" type="integer" column="reserva" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nome" type="string" column="nome" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
