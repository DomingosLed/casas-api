<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Periodo" table="periodo">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="codCasa" type="integer" column="cod_casa" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="inicio" type="string" column="inicio" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fim" type="string" column="fim" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="precoSemana" type="string" column="preco_semana" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="precoDia" type="string" column="preco_dia" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="precoFimsemana" type="string" column="preco_fimsemana" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estadiaMinima" type="string" column="estadia_minima" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descricao" type="string" column="descricao" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="observacoes" type="string" column="observacoes" length="300" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
